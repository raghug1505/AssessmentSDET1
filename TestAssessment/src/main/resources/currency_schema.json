{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["result", "provider", "documentation", "terms_of_use", "base_code", "rates"],
  "properties": {
    "result": {
      "type": "string"
    },
    "provider": {
      "type": "string",
      "format": "uri"
    },
    "documentation": {
      "type": "string",
      "format": "uri"
    },
    "terms_of_use": {
      "type": "string",
      "format": "uri"
    },
    "time_last_update_unix": {
      "type": "integer"
    },
    "time_last_update_utc": {
      "type": "string"
    },
    "time_next_update_unix": {
      "type": "integer"
    },
    "time_next_update_utc": {
      "type": "string"
    },
    "time_eol_unix": {
      "type": "integer"
    },
    "base_code": {
      "type": "string"
    },
    "rates": {
      "type": "object",
      "additionalProperties": {
        "type": "number"
      }
    }
  }
}
